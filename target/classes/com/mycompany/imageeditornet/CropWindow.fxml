<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Rectangle?>

<Pane fx:id="cropPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="com.mycompany.imageeditornet.CropWindowController">
   <children>
      <ImageView fx:id="cropImageView" />
      <Rectangle fx:id="cropRect" fill="TRANSPARENT" stroke="RED" strokeWidth="2" />
      <Button fx:id="cropButton" style="-fx-background-color: orange; -fx-background-radius: 17PX;" text="Crop" />
   </children>
</Pane>
